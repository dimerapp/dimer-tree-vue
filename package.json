{
  "name": "dimer-tree-vue",
  "version": "1.0.0",
  "description": "Converts dimer markdown AST node to Vue components",
  "main": "output.js",
  "files": [
    "output.js"
  ],
  "scripts": {
    "build": "babel index.js -o output.js",
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "japa",
    "commit": "git-cz",
    "lint": "standard"
  },
  "author": "virk",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/mrm-preset": "^1.0.11",
    "@dimerapp/markdown": "^3.1.0",
    "@vue/server-test-utils": "^1.0.0-beta.24",
    "@vue/test-utils": "^1.0.0-beta.24",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "commitizen": "^2.10.1",
    "cz-conventional-changelog": "^2.1.0",
    "dedent": "^0.7.0",
    "japa": "^1.0.6",
    "japa-cli": "^1.0.1",
    "mrm": "^1.2.1",
    "pkg-ok": "^2.2.0",
    "standard": "^12.0.1",
    "vue": "^2.5.17",
    "vue-server-renderer": "^2.5.17",
    "vue-template-compiler": "^2.5.17"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "nyc": {
    "exclude": [
      "test",
      "japaFile.js"
    ]
  },
  "standard": {
    "ignore": [
      "output.js"
    ]
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimerapp/dimer-tree-vue.git"
  },
  "keywords": [
    "dimer"
  ],
  "bugs": {
    "url": "https://github.com/dimerapp/dimer-tree-vue/issues"
  },
  "homepage": "https://github.com/dimerapp/dimer-tree-vue#readme"
}
